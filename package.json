{
  "name": "utu",
  "version": "0.0.1",
  "description": "uTu offical javascript sdk",
  "main": "dist/main.js",
  "scripts": {
    "build": "./node_modules/.bin/babel -d dist/ src/ --no-comments",
    "watch": "./node_modules/.bin/babel -d dist/ src/ --no-comments --watch",
    "test": "./node_modules/.bin/mocha --compilers js:babel-core/register --recursive ./tests",
    "watch-test": "./node_modules/.bin/mocha --compilers js:babel-core/register --recursive ./tests --watch",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/utu-ai/utu-javascript-sdk.git"
  },
  "author": "uTu <dev@utu.ai>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/utu-ai/utu-javascript-sdk/issues"
  },
  "homepage": "https://github.com/utu-ai/utu-javascript-sdk#readme",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-watch": "^2.0.3",
    "chai": "^3.5.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.0",
    "mocha": "^3.1.2",
    "nock": "^9.0.2"
  },
  "dependencies": {
    "es6-promise": "^4.0.5",
    "isomorphic-fetch": "^2.2.1"
  },
  "eslintConfig": {
    "extends": [
      "airbnb/base"
    ],
    "rules": {
      "import/no-unresolved": 0,
      "func-names": 0
    }
  }
}
